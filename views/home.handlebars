<div class="">

  {{#each posts as |post|}}
  <div class="col-md-4 border border-black">
    <h3>
      {{post.title}}
    </h3>
    <p>
      {{post.content}}
    </p>
    <p class="">
      Author:
      {{post.user.username}}
    </p>
  </div>
  {{/each}}

  <script src="/js/add-post.js"></script>